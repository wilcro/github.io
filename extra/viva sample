        <script type="text/javascript">

            $(document).ready(function() {

                $("#MobileMenu").mmenu({
                    offCanvas: {
                        position: "left"
                    }
                });

                $("#CloseMC").click(function() {
                    $("#MobileMenu").trigger("close.mm");
                });

                // Search form ...
                $(function() {
                    $('.fa-search').on('click touch', function() {
                        if (!$('body').hasClass('show-search')) {
                            $('body').addClass('show-search');
                            setTimeout(function() {
                                $('#SearchForm').css('display', 'block');
                            }, 100);
                        }
                        return false;
                    });
                    $('#SearchForm .fa-close').on('click touch', function() {
                        if ($('body').hasClass('show-search')) {
                            $('body').removeClass('show-search');
                            setTimeout(function() {
                                $('#SearchForm').css('display', 'none');
                            }, 100);
                        }
                        return false;
                    });
                });

                /* Sticky header nav */

                var header = document.querySelector('header');
                var body = document.querySelector('body');
                var origOffsetY = header.offsetTop;

                function onScroll(e) {
                    window.scrollY >= origOffsetY ? header.classList.add('sticky') : header.classList.remove('sticky');

                    window.scrollY >= origOffsetY ? body.classList.add('sticky-body') : body.classList.remove('sticky-body');
                }

                document.addEventListener('scroll', onScroll);

            });
        </script>
